<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
    <metadata>
        <id>Reminiscence</id>
        <version>$version$</version>
        <title>Reminiscence</title>
        <authors>Ben Abelshausen</authors>
        <owners>Ben Abelshausen</owners>
        <licenseUrl>https://raw.githubusercontent.com/itinero/reminiscence/master/license.md</licenseUrl>
        <projectUrl>https://github.com/itinero/reminiscence</projectUrl>
        <iconUrl>https://raw.githubusercontent.com/itinero/reminiscence/master/nuspec/icon.png</iconUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Simple cross-platform memory mapping for .NET/Mono.</description>
        <summary>Reminiscence is a lightweight cross platform memory mapping library to bring some form of memory mapping to .NET on any platform.
        </summary>
        <copyright>Ben Abelshausen</copyright>
        <tags>memory, memorymapping, collections</tags>
        <dependencies>
